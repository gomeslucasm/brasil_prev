              "schema": { "$ref": "#/components/schemas/PlanoAporteExtra" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PlanoOperationResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/api/planos/retirada": {
      "post": {
        "summary": "Retirada Plano",
        "operationId": "retirada_plano_api_planos_retirada_post",
        "requestBody": {
          "content": {
