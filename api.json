  "components": {
    "schemas": {
      "ClientCreate": {
        "properties": {
          "cpf": { "type": "string", "title": "Cpf" },
          "nome": { "type": "string", "title": "Nome" },
          "email": { "type": "string", "format": "email", "title": "Email" },
          "dataDeNascimento": {
            "type": "string",
            "format": "date-time",
            "title": "Datadenascimento"
          },
          "genero": { "type": "string", "title": "Genero" },
          "rendaMensal": { "type": "number", "title": "Rendamensal" }
        },
        "type": "object",
        "required": [
          "cpf",
          "nome",
          "email",
          "dataDeNascimento",
          "genero",
          "rendaMensal"
        ],
        "title": "ClientCreate"
      },
      "ClientResponse": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" }
        },
        "type": "object",
        "required": ["id"],
        "title": "ClientResponse"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "PlanoAporteExtra": {
        "properties": {
          "idPlano": { "type": "string", "format": "uuid", "title": "Idplano" },
          "valorAporte": { "type": "number", "title": "Valoraporte" }
        },
        "type": "object",
        "required": ["idPlano", "valorAporte"],
        "title": "PlanoAporteExtra"
      },
      "PlanoCreate": {
        "properties": {
          "idCliente": {
            "type": "string",
            "format": "uuid",
            "title": "Idcliente"
          },
          "idProduto": {
            "type": "string",
            "format": "uuid",
            "title": "Idproduto"
          },
          "aporte": { "type": "number", "title": "Aporte" },
          "dataDaContratacao": {
            "type": "string",
            "format": "date-time",
            "title": "Datadacontratacao"
          },
          "idadeDeAposentadoria": {
            "type": "integer",
            "title": "Idadedeaposentadoria"
          }
        },
        "type": "object",
        "required": [
          "idCliente",
          "idProduto",
          "aporte",
          "dataDaContratacao",
          "idadeDeAposentadoria"
        ],
        "title": "PlanoCreate"
      },
      "PlanoOperationResponse": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" }
        },
        "type": "object",
        "required": ["id"],
        "title": "PlanoOperationResponse"
      },
      "PlanoResponse": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" }
        },
        "type": "object",
        "required": ["id"],
        "title": "PlanoResponse"
      },
      "PlanoRetirada": {
        "properties": {
          "idPlano": { "type": "string", "format": "uuid", "title": "Idplano" },
          "valorResgate": { "type": "number", "title": "Valorresgate" }
        },
        "type": "object",
        "required": ["idPlano", "valorResgate"],
        "title": "PlanoRetirada"
      },
      "ProdutoCreate": {
        "properties": {
          "nome": { "type": "string", "title": "Nome" },
          "susep": { "type": "string", "title": "Susep" },
          "expiracaoDeVenda": {
            "type": "string",
            "format": "date-time",
            "title": "Expiracaodevenda"
          },
          "valorMinimoAporteInicial": {
            "type": "number",
            "title": "Valorminimoaporteinicial"
          },
          "valorMinimoAporteExtra": {
            "type": "number",
            "title": "Valorminimoaporteextra"
          },
          "idadeDeEntrada": { "type": "integer", "title": "Idadedeentrada" },
          "idadeDeSaida": { "type": "integer", "title": "Idadedesaida" },
          "carenciaInicialDeResgate": {
            "type": "integer",
            "title": "Carenciainicialderesgate"
          },
          "carenciaEntreResgates": {
            "type": "integer",
            "title": "Carenciaentreresgates"
          }
        },
        "type": "object",
        "required": [
          "nome",
          "susep",
          "expiracaoDeVenda",
          "valorMinimoAporteInicial",
          "valorMinimoAporteExtra",
          "idadeDeEntrada",
          "idadeDeSaida",
          "carenciaInicialDeResgate",
          "carenciaEntreResgates"
        ],
        "title": "ProdutoCreate"
      },
      "ProdutoResponse": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" }
        },
        "type": "object",
        "required": ["id"],
        "title": "ProdutoResponse"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
